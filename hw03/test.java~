public class test{
   
 
 public static int compareTo(int[] A, int[] B) {
  
   if (A.length==B.length){
       System.out.println(1);
       if (A[0] >B[0])
         System.out.println(1);
   }
   
    if (A.length < B.length){
      return -1;
    }
   if (A.length > B.length){
      return 1;
    }
  if(A.length == B.length){
      for(int i =0; i < A.length-1;++i){
        if(A[i]>B[i]){
          return 1;
      
      }
      }
  }

      
        return 0;
  }




public static void main(String [] args) {
  int [] a ={1};
  int [] b ={0};
  int c = compareTo(a,b);
  System.out.println(c);
}
}